                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      context="tei:p[@hand] | tei:div[@hand]">
    	                <sch:report test="@hand" role="warning">Please check if this is a paragraph an not an addition (&lt;add&gt;)</sch:report>
    	            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
