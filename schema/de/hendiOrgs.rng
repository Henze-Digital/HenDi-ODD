                       xmlns:xi="http://www.w3.org/2001/XInclude"
                       xmlns:hendi="http://henze-digital.zenmem.de/ns/1.0"
                       xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       test="@n='extent' and not(parent::tei:physDesc)">
                        The value 'extent' must only be used within the tei:physDesc.
                    </report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="hendiOrgs-p-extent-only-once-constraint-rule-13">
            <sch:rule xmlns:mei="http://www.music-encoding.org/ns/mei"
